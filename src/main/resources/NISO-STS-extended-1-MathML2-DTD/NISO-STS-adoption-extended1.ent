<!--                    ADOPTION WRAPPER ATTRIBUTES                -->
<!--                    Attributes used with the <adoption> element,
                        which is a document-level element that
                        contains its own material and one or more
                        <standard> elements.                       -->
<!ENTITY % adoption-atts
            "%jats-common-atts;  
             %dtd-version;
             specific-use
                        CDATA                             #IMPLIED
             xml:lang   NMTOKEN                           'en'
             %XLINK.xmlns.attrib;
             %MATHML.xmlns.attrib;
             %xinclude.xmlns.attrib;
             %ali.xmlns.attrib;
             %Schema.xmlns.attrib;
             %oasis.xmlns.attrib;
             %TBX.xmlns.attrib;"                                     >


