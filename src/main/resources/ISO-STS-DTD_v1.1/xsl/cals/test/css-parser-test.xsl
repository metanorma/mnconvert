    <xsl:variable name="test-cases">
        <xsl:for-each select="tokenize($css-lines, '\n')">
            <xsl:if test="normalize-space(.)">
                <style><xsl:value-of select="normalize-space(.)"/></style>
            </xsl:if>
        </xsl:for-each>
    </xsl:variable>
    <xsl:template match="/">
        <html>
            <head>
                <title>Test cases for CSS parser</title>
            </head>
            <body>
                <h2>Test cases for CSS parser</h2>
                <table border="1" cellpadding="3">
                    <thead>
                        <tr>
                            <th>CSS definition</th>
                            <th>Parse OK</th>
                            <th>Error message</th>
                        </tr>
                    </thead>
                    <tbody>
                        <xsl:for-each select="$test-cases/style">
                            <xsl:variable name="css" select="css:parse(.)"/>
                            <tr>
                                <td>
                                    <xsl:value-of select="."/>
                                </td>
                                <td>
                                    <xsl:if test="exists($css//css:error)">
                                        <xsl:attribute name="style">background-color: red</xsl:attribute>
                                    </xsl:if>
                                    <xsl:value-of select="not(exists($css//css:error))"/>
                                </td>
                                <td>
                                    <xsl:for-each select="$css//css:error">
                                        <xsl:value-of select="."/>
                                        <br/>
                                    </xsl:for-each>
                                </td>
                            </tr>
                        </xsl:for-each>
                    </tbody>
                </table>
            </body>
        </html>
    </xsl:template>
</xsl:stylesheet>
